    "body": [
      "function BasicUsage() {",
      "\tconst { isOpen, onOpen, onClose } = useDisclosure();",
      "\treturn (",
      "\t\t<>",
      "\t\t\t<Button onClick={onOpen}>Open Modal</Button>",
      "",
