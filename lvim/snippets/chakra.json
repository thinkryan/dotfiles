      "\t\tborderWidth=\"1px\"",
      "\t\tsize=\"24px\"",
      "\t\trounded=\"sm\"",
      "\t\t_checked={{ bg: \"green.500\", color: \"white\", borderColor: \"green.500\" }}",
      "\t\t_focus={{ borderColor: \"green.600\", boxShadow: \"outline\" }}",
      "\t>",
      "\t\t<Icon name=\"check\" size=\"16px\" />",
