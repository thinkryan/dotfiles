  "Drawer": {
    "prefix": "drawer",
    "description": "⚡️ Chakra UI: Example Drawer Component",
    "body": [
      "function DrawerExample() {",
      "\tconst { isOpen, onOpen, onClose } = useDisclosure();",
      "\tconst btnRef = React.useRef();",
      "",
      "\treturn (",
      "\t\t<>",
      "\t\t\t<Button ref={btnRef} variantColor=\"teal\" onClick={onOpen}>",
      "\t\t\t\tOpen",
      "\t\t\t</Button>",
      "\t\t\t<Drawer",
      "\t\t\t\tisOpen={isOpen}",
      "\t\t\t\tplacement=\"right\"",
      "\t\t\t\tonClose={onClose}",
      "\t\t\t\tfinalFocusRef={btnRef}",
      "\t\t\t>",
      "\t\t\t\t<DrawerOverlay />",
      "\t\t\t\t<DrawerContent>",
      "\t\t\t\t\t<DrawerCloseButton />",
      "\t\t\t\t\t<DrawerHeader>Create your account</DrawerHeader>",
      "",
      "\t\t\t\t\t<DrawerBody>",
      "\t\t\t\t\t\t<Input placeholder=\"Type here...\" />",
      "\t\t\t\t\t</DrawerBody>",
      "",
      "\t\t\t\t\t<DrawerFooter>",
      "\t\t\t\t\t\t<Button variant=\"outline\" mr={3} onClick={onClose}>",
      "\t\t\t\t\t\t\tCancel",
      "\t\t\t\t\t\t</Button>",
      "\t\t\t\t\t\t<Button color=\"blue\">Save</Button>",
      "\t\t\t\t\t</DrawerFooter>",
      "\t\t\t\t</DrawerContent>",
      "\t\t\t</Drawer>",
      "\t\t</>",
      "\t);",
      "}"
    ]
  },
